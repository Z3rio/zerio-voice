{
  "logging": {
    "level": "info"
  },

  "voiceModes": [
    {
      "range": 1.5,
      "name": "Whisper"
    },
    {
      "range": 3.0,
      "name": "Normal",
      "default": true
    },
    {
      "range": 6.0,
      "name": "Shouting"
    }
  ],

  "proximity": {
    "keybind": "F10"
  },

  "ui": {
    "enabled": true,

    "radioMemberList": {
      "enabled": true,
      "showMembersOfAllChannels": false
    },

    "interaction": {
      "key": "LMenu",
      "requireMousePressAswell": true
    }
  },

  "framework": {
    "customResourceName": null
  },

  "locale": {
    "language": "en"
  },

  "submix": {
    "radio": true,
    "call": true
  },

  "radio": {
    "enabled": true,
    "enableMicClicks": true,
    "micClicksVolume": 0.6,
    "keybind": "CAPITAL",
    "noTalkOverMode": false
  }
}
